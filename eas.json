{
  "cli": {
    "version": ">= 5.0.0"
  },
  "build": {
    "preview": {
      "android": {
        "buildType": "apk",
        "env": {
          "EXPO_PUBLIC_ENV": "preview"
        }
      },
      "ios": {
        "simulator": true,
        "env": {
          "EXPO_PUBLIC_ENV": "preview"
        }
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle",
        "env": {
          "EXPO_PUBLIC_ENV": "production"
        }
      },
      "ios": {
        "env": {
          "EXPO_PUBLIC_ENV": "production"
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}
